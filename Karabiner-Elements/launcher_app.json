{
    "title":"Launcher app",
    "description": "Launcher app rules for Chrome and GoLand",
    "manipulators": [
        {
            "from": {
                "key_code": "1",
                "modifiers": { "mandatory": ["left_shift", "command"] }
            },
            "to": [
                {
                  "software_function": {
                    "open_application": {
                      "bundle_identifier": "com.google.Chrome"
                    }
                  }
                }
            ],
            "type": "basic"
        },
        {
            "from": {
                "key_code": "2",
                "modifiers": { "mandatory": ["left_shift", "command"] }
            },
            "to": [
                {
                  "software_function": {
                    "open_application": {
                      "bundle_identifier": "com.jetbrains.goland"
                    }
                  }
                }
            ],
            "type": "basic"
        }
    ]
}